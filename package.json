{
  "name": "webpack-template",
  "version": "0.1.0",
  "description": "A webpack template.",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "webpack --env.production --config webpack.config.js --progress",
    "analyzer": "webpack --env.production --env.analyzer --config webpack.config.js --progress",
    "lint": "eslint --fix src --ext .js,.jsx,.ts,.tsx"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "keywords": [],
  "author": "horochx",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^10.14.5",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-loader": "^8.0.5",
    "cache-loader": "^2.0.1",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "thread-loader": "^2.1.2",
    "ts-loader": "^5.4.3",
    "typescript": "^3.4.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.4",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "babel-plugin-react-css-modules": "^5.2.4",
    "core-js": "^3.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "regenerator-runtime": "^0.13.2"
  }
}
